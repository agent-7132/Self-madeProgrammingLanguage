[memory_safety]
quantum_entanglement = restricted
classic_memory_access = validated

[code_generation]
allow_unsafe_optimizations = false
max_qubit_usage = 1024

[cryptography]
required_algorithms = dilithium3
min_key_length = 256

[verification]
formal_verification_required = true
alloy_max_trace = 5

[quantum_runtime]
max_circuit_depth = 1000
error_threshold = 1e-3
entanglement_monitoring = true

[container_security]
memory_limit = 4G
cpu_quota = 75%
